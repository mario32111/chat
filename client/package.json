{
  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./public/css/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/input.css -o ./public/css/output.css --minify",
    "dev1": "set NODE_ENV=development && node server.js",
    "dev2": "set NODE_ENV=development && set PORT=8081 && node server.js",
    "dev": "set NODE_ENV=development && nodemon server.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "firebase-admin": "^13.5.0",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "livereload": "^0.10.3"
  }
}